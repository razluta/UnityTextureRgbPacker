        <ui:VisualElement name="VE_BlueChannelInputRow" class="single_input_row">
            <ui:Toggle class="single_input_row_toggle thin_border" style="background-color: rgb(18, 58, 102);" />
            <ui:VisualElement name="VE_BlueChannelInput" class="single_channel_input thin_border" style="margin-left: 0; background-color: rgb(18, 58, 102);">
