            <ui:Toggle class="thin_border single_input_row_toggle" style="background-color: rgb(102, 18, 18);" />
            <ui:VisualElement name="VE_RedChannelInput" class="single_channel_input thin_border" style="background-color: rgba(102, 18, 18, 255);">
                <ui:Label text="Red Channel:" class="single_input_row_label" />
                <uie:ObjectField name="OF_RedChannelTexture" class="single_input_row_texture_input" />
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement name="VE_GreenChannelInputRow" class="single_input_row">
            <ui:Toggle class="thin_border single_input_row_toggle" style="background-color: rgb(49, 102, 18);" />
            <ui:VisualElement name="VE_GreenChannelInput" class="single_channel_input thin_border" style="background-color: rgb(49, 102, 18);">
                <ui:Label text="Green Channel:" class="single_input_row_label" />
                <uie:ObjectField name="OF_GreenChannelTexture" class="single_input_row_texture_input" />
